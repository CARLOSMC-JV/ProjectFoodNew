{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
          "distDir": "public/build/assets"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "destination": "/"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}